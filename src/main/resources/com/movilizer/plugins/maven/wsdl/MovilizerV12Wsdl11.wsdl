				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="8" minOccurs="0" name="masterdata" type="movilizer:MovilizerMoveletMasterdata" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="8" minOccurs="0" name="document" type="movilizer:MovilizerMoveletDocument" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="onMasterdataChange" type="movilizer:MovilizerMoveletMasterdataChange" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="encryptionIV" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="encryptionHMAC" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
