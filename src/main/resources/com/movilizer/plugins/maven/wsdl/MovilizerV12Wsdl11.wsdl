			<xsd:element maxOccurs="64" minOccurs="0" name="useParameter" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
					<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Specify which parameters to use in the reply. Allows 8 parameters max.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0" name="defineParameter" type="movilizer:MovilizerParameterDescriptor" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
					<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Initiates a cyclic question. The parameter that has to be filled with the selected answers value. Works only with SingleSelectQuestions(type=2).</xsd:documentation>
				</xsd:annotation>
