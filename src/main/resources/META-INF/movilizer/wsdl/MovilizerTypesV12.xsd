        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="namespace" type="movilizer:clientKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" default="true" name="privateNamespace" type="xsd:boolean"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">If set to true, the namespace will only
                    be available within the same system.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="icon" type="xsd:byte" use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="hotkey" type="movilizer:string15"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="moveletType" type="movilizer:moveletType"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="metaMoveletType" type="movilizer:metaMoveletType"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="initialQuestionKey" type="movilizer:nativeKey"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" default="false" name="transactional" type="xsd:boolean"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="validTillDate" type="xsd:dateTime"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="encryptionAlgorithm" type="movilizer:string15"
