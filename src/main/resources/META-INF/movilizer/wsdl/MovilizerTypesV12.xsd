                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A MEL assignment that is triggered
                        right before the Movelet gets deleted from the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="syncDownloadAssignment"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A MEL assignment that is triggered
                        right after the Movelet was downloaded on the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="moveletStartAssignment"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A MEL expression that is executed
                        whenever the Movelet is started or resumed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="clientStartAssignment"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A MEL expression that is executed
                        whenever the Movilizer client starts.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="1" name="name"
                         type="movilizer:string255">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Movelet name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="nameExpression"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A dynamic MEL expression that allows
                        a dynamic representation of the Movelet name in the list of Movelets on the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="iconExpression"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A dynamic MEL expression that allows
                        a dynamic representation of the Movelet icon in the list of Movelets on the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="visibleExpression"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">A dynamic MEL expression that
                        controlls if the Movelet will be displayed in the Movelet list on the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="priorityExpression"
                         type="xsd:string">
                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">An dynamic MEL expression that
                        allows a dynamic ordering of the Movelets in the Movelet list on the client.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded" minOccurs="0" name="category"
                         type="movilizer:MovilizerMoveletCategory"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0"
                         name="tableColumnDefinition"
                         type="movilizer:MovilizerMoveletTableColumnDefinition">
