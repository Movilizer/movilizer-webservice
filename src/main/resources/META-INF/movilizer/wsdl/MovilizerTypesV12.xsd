                 type="movilizer:MovilizerMovelet"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerMovelet">
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="metaMovelet"
                         type="movilizer:MovilizerMetaMovelet"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded" minOccurs="1" name="question"
                         type="movilizer:MovilizerQuestion"/>

