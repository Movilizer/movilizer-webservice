                <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Meta information that comes with the
                        movelet - presented in movelet grid.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="8" minOccurs="0" name="masterdata"
                         type="movilizer:MovilizerMoveletMasterdata"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="8" minOccurs="0" name="document"
                         type="movilizer:MovilizerMoveletDocument"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0"
                         name="onMasterdataChange"
                         type="movilizer:MovilizerMoveletMasterdataChange"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="encryptionIV"
                         type="movilizer:nativeKey"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="encryptionHMAC"
                         type="movilizer:nativeKey"/>
        </xsd:sequence>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="moveletKey" type="movilizer:nativeKey"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="moveletKeyExtension" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="responseQueue" type="movilizer:string7"
