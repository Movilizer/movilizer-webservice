        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xsd:dateTime"
                       use="required"/>
    </xsd:complexType>

    <!-- Response: Participant deployment info -->
    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantDeploymentSmsSent"
                 type="movilizer:MovilizerParticipantDeploymentSmsSent"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantDeploymentSmsSent">
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="deviceAddress" type="movilizer:deviceAddress"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="messageId" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xsd:dateTime"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="isMail" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstall"
                 type="movilizer:MovilizerParticipantInstall"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstall">
        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Indicates that the mobile phone downloaded
                the application.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="participantKey" type="movilizer:nativeKey"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The participantKey can be empty if
                    assigned outside the context of a movelet via a standalone participant command.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="deviceAddress" type="movilizer:deviceAddress"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="ipAddress" type="movilizer:string15"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xsd:dateTime"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="userAgent" type="movilizer:string1023"
                       use="required"/>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstallAck"
                 type="movilizer:MovilizerParticipantInstallAck"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstallAck">
        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Indicates that the mobile phone downloaded
                the application.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="participantKey" type="movilizer:nativeKey"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The participantKey can be empty if
                    assigned outside the context of a movelet via a standalone participant command.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="deviceAddress" type="movilizer:deviceAddress"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="ipAddress" type="movilizer:string15"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xsd:dateTime"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="userAgent" type="movilizer:string1023"
                       use="required"/>
    </xsd:complexType>


    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstallError"
                 type="movilizer:MovilizerParticipantInstallError"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerParticipantInstallError">
        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Indicates that the mobile phone downloaded
                the application.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="participantKey" type="movilizer:nativeKey"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The participantKey can be empty if
                    assigned outside the context of a movelet via a standalone participant command.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="deviceAddress" type="movilizer:deviceAddress"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="ipAddress" type="movilizer:string15"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xsd:dateTime"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="errorCode" type="movilizer:string15"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="userAgent" type="movilizer:string1023"
                       use="required"/>
    </xsd:complexType>


    <!-- Response: Reply -->
    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyMovelet"
                 type="movilizer:MovilizerReplyMovelet"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyMovelet">
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded" minOccurs="0"
                         name="replyQuestion" type="movilizer:MovilizerReplyQuestion"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="encryptionIV"
                         type="movilizer:nativeKey"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="encryptionHMAC"
                         type="movilizer:nativeKey"/>
        </xsd:sequence>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="moveletKey" type="movilizer:nativeKey"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="moveletKeyExtension" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" default="0" name="moveletVersion" type="xsd:long"
                       use="optional"/>

        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="count" type="xsd:int" use="required"/>

        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="parentMoveletKey" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="parentMoveletKeyExtension" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" default="0" name="parentMoveletVersion" type="xsd:long"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="parentReplySetCount" type="xsd:int"
                       use="optional"/>

        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="encryptionAlgorithm" type="movilizer:string15"
                       use="optional"/>

        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="participantKey" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="deviceAddress" type="movilizer:deviceAddress"
                       use="required"/>

        <!-- <xsd:attribute name="isComplete" type="xsd:boolean" use="required" /> -->
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="creationTimestamp" type="xsd:dateTime"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The client-time when the replySet was
                    created on the mobile device.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="syncTimestamp" type="xsd:dateTime"
                       use="required">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The server-time when the replySet was
                    synced to the server.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="timedif" type="xsd:int" use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The time difference between Server and
                    Client in seconds: timedif = serverTime - clientTime
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" default="0" name="replyUploadPriority" type="xsd:short"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Priority that controls the order of
                    reply-upload. Higher value means higher priority.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyQuestion"
                 type="movilizer:MovilizerReplyQuestion"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyQuestion">
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded" minOccurs="0" name="replyAnswer"
                         type="movilizer:MovilizerReplyAnswer"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="64" minOccurs="0"
                         name="parameterValue" type="movilizer:MovilizerReplyParameterValue"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="encryptionIV"
                         type="movilizer:nativeKey"/>
        </xsd:sequence>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="questionKey" type="movilizer:nativeKey"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="previousQuestionKey" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="nextQuestionKey" type="movilizer:nativeKey"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="creationTimestamp" type="xsd:dateTime"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="questionCountNormalized" type="xsd:int"
                       use="optional"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="questionCountAbsolute" type="xsd:int"
                       use="optional"/>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyAnswer"
                 type="movilizer:MovilizerReplyAnswer"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyAnswer">
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="value"
                         type="movilizer:string1023"/>
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="0" name="data"
                         type="movilizer:binary"/>
        </xsd:sequence>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="answerKey" type="movilizer:nativeKey"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The answerkey might be empty for certain
                    questiontypes. E.g. if nothing is selected in a multiselect question
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="attributeType" type="xsd:byte" use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The attribute type might be empty for
                    certain questiontypes. E.g. if the question is not of type multi text item screen
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="position" type="xsd:short" use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Contains the answers position.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="clientKey" type="movilizer:clientKey"
                       use="optional"/>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyParameterValue"
                 type="movilizer:MovilizerReplyParameterValue"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerReplyParameterValue">
        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Only one value can be set at a time,
                depending on which parameterType has been selected in the Movelet-definition.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="parameterKey" type="movilizer:nativeKey"
                       use="required"/>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="valueAnswerKey" type="movilizer:nativeKey"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The value field that is being used if
                    the ParameterType is set to AnswerKey.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="valueFreeText" type="movilizer:string1023"
                       use="optional">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The value field that is being used if
                    the ParameterType is set to any other type.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>


    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerRequest">
        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:complexContent xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" base="movilizer:MovilizerRequestAbstract">
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <!-- Movilizer Online-Container structure to exchange data outside the Movelet / Response cycle -->
    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerOnlineResponse">
        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:complexContent xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:extension xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                               base="movilizer:MovilizerRequestAbstract">
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerOnlineDataContainer"
                 type="movilizer:MovilizerOnlineDataContainer"/>
    <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerOnlineDataContainer">
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="1" minOccurs="1"
                         name="container"
                         type="movilizer:MovilizerGenericUploadDataContainer"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MovilizerOnlineCallbackRequest">
        <xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                    Movilizer Online Request which is send from within a Movelet. Is retrieving a response a of
                    type
                    MovilizerRequest which may process all kind of objects as a normal request coming in
                </xsd:documentation>
            </xsd:annotation>
            <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <xsd:element xmlns:xsd="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded" minOccurs="0"
                             name="onlineContainer"
                             type="movilizer:MovilizerOnlineDataContainer"/>
            </xsd:sequence>
            <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="systemId" type="xsd:long"
                           use="required"/>
            <xsd:attribute xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="category" type="movilizer:string31"
                           use="optional"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>