	<xsd:element name="MovilizerReplyMovelet" type="movilizer:MovilizerReplyMovelet" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	<xsd:complexType name="MovilizerReplyMovelet" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="replyQuestion" type="movilizer:MovilizerReplyQuestion" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="encryptionIV" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="encryptionHMAC" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		</xsd:sequence>
		<xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute default="0" name="moveletVersion" type="xsd:long" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>

		<xsd:attribute name="count" type="xsd:int" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>

		<xsd:attribute name="parentMoveletKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="parentMoveletKeyExtension" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute default="0" name="parentMoveletVersion" type="xsd:long" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="parentReplySetCount" type="xsd:int" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>

		<xsd:attribute name="encryptionAlgorithm" type="movilizer:string15" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>

		<xsd:attribute name="participantKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>

		<!-- <xsd:attribute name="isComplete" type="xsd:boolean" use="required" /> -->
		<xsd:attribute name="creationTimestamp" type="xsd:dateTime" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The client-time when the replySet was created on the mobile device.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="syncTimestamp" type="xsd:dateTime" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The server-time when the replySet was synced to the server.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="timedif" type="xsd:int" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The time difference between Server and Client in seconds: timedif = serverTime - clientTime</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute default="0" name="replyUploadPriority" type="xsd:short" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Priority that controls the order of reply-upload. Higher value means higher priority.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:element name="MovilizerReplyQuestion" type="movilizer:MovilizerReplyQuestion" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	<xsd:complexType name="MovilizerReplyQuestion" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="replyAnswer" type="movilizer:MovilizerReplyAnswer" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="64" minOccurs="0" name="parameterValue" type="movilizer:MovilizerReplyParameterValue" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="encryptionIV" type="movilizer:nativeKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		</xsd:sequence>
		<xsd:attribute name="questionKey" type="movilizer:nativeKey" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="previousQuestionKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="nextQuestionKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="creationTimestamp" type="xsd:dateTime" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="questionCountNormalized" type="xsd:int" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="questionCountAbsolute" type="xsd:int" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	</xsd:complexType>

	<xsd:element name="MovilizerReplyAnswer" type="movilizer:MovilizerReplyAnswer" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	<xsd:complexType name="MovilizerReplyAnswer" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element maxOccurs="1" minOccurs="0" name="value" type="movilizer:string1023" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="data" type="movilizer:binary" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		</xsd:sequence>
		<xsd:attribute name="answerKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The answerkey might be empty for certain questiontypes. E.g. if nothing is selected in a multiselect question</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="attributeType" type="xsd:byte" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The attribute type might be empty for certain questiontypes. E.g. if the question is not of type multi text item screen</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="position" type="xsd:short" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Contains the answers position.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="clientKey" type="movilizer:clientKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	</xsd:complexType>

	<xsd:element name="MovilizerReplyParameterValue" type="movilizer:MovilizerReplyParameterValue" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	<xsd:complexType name="MovilizerReplyParameterValue" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema"><xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">Only one value can be set at a time, depending on which parameterType has been selected in the Movelet-definition.</xsd:documentation></xsd:annotation>
		<xsd:attribute name="parameterKey" type="movilizer:nativeKey" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		<xsd:attribute name="valueAnswerKey" type="movilizer:nativeKey" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema"><xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The value field that is being used if the ParameterType is set to AnswerKey.</xsd:documentation></xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="valueFreeText" type="movilizer:string1023" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema"><xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">The value field that is being used if the ParameterType is set to any other type.</xsd:documentation></xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>


	<xsd:element name="MovilizerRequest" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:complexContent xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:extension base="movilizer:MovilizerRequestAbstract" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

    <!-- Movilizer Online-Container structure to exchange data outside the Movelet / Response cycle -->
	<xsd:element name="MovilizerOnlineResponse" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:complexContent xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:extension base="movilizer:MovilizerRequestAbstract" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="MovilizerOnlineDataContainer" type="movilizer:MovilizerOnlineDataContainer" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
	<xsd:complexType name="MovilizerOnlineDataContainer" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element maxOccurs="1" minOccurs="1" name="container" type="movilizer:MovilizerGenericUploadDataContainer" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="MovilizerOnlineCallbackRequest" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
		<xsd:complexType xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:documentation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
					Movilizer Online Request which is send from within a Movelet. Is retrieving a response a of type
					MovilizerRequest which may process all kind of objects as a normal request coming in
				</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
				<xsd:element maxOccurs="unbounded" minOccurs="0" name="onlineContainer" type="movilizer:MovilizerOnlineDataContainer" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			</xsd:sequence>
			<xsd:attribute name="systemId" type="xsd:long" use="required" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
			<xsd:attribute name="category" type="movilizer:string31" use="optional" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>